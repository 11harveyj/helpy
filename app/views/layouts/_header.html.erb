<div id="header-wrapper">
			<div id="above-header">
				<%= render :partial => 'layouts/above_header' %>
	    </div>
			<%= nav_bar brand: "#{Settings.product_name + " " if Settings.product_name}" "#{Settings.site_name}", responsive: true do %>
					<%= menu_group pull: :right do %>
							<%#= menu_item "Home", root_path %>
							<% unless Settings.parent_company.blank? %>
							<%= menu_item "Back to #{Settings.parent_company}", "#{Settings.parent_site}" %>
							<% end %>
							<%= menu_item "Knowledgebase", categories_path, class:'kblink' if Settings.knowledgebase %>
							<%= menu_item "Forums", forums_path if Settings.forums %>
							<%= menu_item "Ask a Question", new_topic_path %>
							<%= menu_item "Search", result_path %>
					<% end %>
			<% end %>
</div>
